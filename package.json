{
  "name": "restapi",
  "version": "2.0.0",
  "description": "Node JS Rest API V2",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run www",
    "www": "node ./bin/www",
    "dev": "nodemon --ext ts --watch src --exec './node_modules/.bin/ts-node' src/bin/www.ts",
    "build": "node ./bin/build",
    "build-readme": "./node_modules/.bin/ts-node readme.ts",
    "pretest": "npm run build",
    "test": "./node_modules/.bin/mocha --exit"
  },
  "keywords": [
    "rest",
    "rest api"
  ],
  "author": "Soe Minn Minn",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.4",
    "knex": "^0.16.3",
    "mysql": "^2.16.0",
    "sqlite3": "^4.0.6"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/knex": "^0.15.1",
    "@types/mysql": "^2.15.5",
    "@types/node": "^11.11.0",
    "@types/shelljs": "^0.8.1",
    "@types/showdown": "^1.9.0",
    "@types/sqlite3": "^3.1.5",
    "mocha": "^6.0.2",
    "shelljs": "^0.8.3",
    "showdown": "^1.9.1",
    "supertest": "^4.0.0",
    "ts-node": "^8.0.3",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3333"
  }
}
